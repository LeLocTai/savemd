{
  "name": "Awesome Extension",
  "version": "0.0.0",
  "description": "An awesome new browser extension",
  "homepage_url": "https://github.com/awesome-templates/browser-extension-template",
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "awesome-extension@notlmn.github.io",
      "strict_min_version": "67.0"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs",
    "downloads",
    "storage",
    "contextMenus",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_title": "SaveMD",
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.ts"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.ts"
      ]
    }
  ]
}